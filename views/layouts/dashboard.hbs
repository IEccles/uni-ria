<script>
    document.addEventListener('DOMContentLoaded', () => {
        // Extender buttons
        const sidebar = document.getElementById('sidebar');
        const warehouseBtn = document.getElementById('extender-warehouse');
        //const purchasingBtn = document.getElementById('extender-purchasing');
        const crmBtn = document.getElementById('extender-crm');
        //const insightBtn = document.getElementById('extender-insight');
        const settingsBtn = document.getElementById('extender-settings');

        // Extender menus
        const warehouseMenu = document.getElementById('extended-warehouse');
        //const purchasingMenu = document.getElementById('extended-purchasing');
        const crmMenu = document.getElementById('extended-crm');
        //const insightMenu = document.getElementById('extended-insight');
        const settingsMenu = document.getElementById('extended-settings');

        // Mobile sidebar button
        const openSidebar = document.getElementById('open-sidebar');
        const blackOut = document.getElementById('mobile-sidebar-blackout');

        const open = (menu) => {
            warehouseMenu.classList.add('hidden');
            //purchasingMenu.classList.add('hidden');
            crmMenu.classList.add('hidden');
            //insightMenu.classList.add('hidden');
            settingsMenu.classList.add('hidden');

            if (menu) {
                menu.classList.remove('hidden');
            }
        }

        // Event listeners
        warehouseBtn.addEventListener('click', () => open(warehouseMenu));
        //purchasingBtn.addEventListener('click', () => open(purchasingMenu));
        crmBtn.addEventListener('click', () => open(crmMenu));
        //insightBtn.addEventListener('click', () => open(insightMenu));
        settingsBtn.addEventListener('click', () => open(settingsMenu));

        // Close all menus when clicked outside sidebar
        document.addEventListener('click', (e) => {
            if (!sidebar.contains(e.target)) {
                open();
            }

            if (!sidebar.parentElement.classList.contains('hidden')) {
                if (blackOut.contains(e.target)) {
                    sidebar.parentElement.classList.add('hidden');
                    blackOut.classList.add('hidden');
                }
            }
        });

        openSidebar.addEventListener('click', () => {
            sidebar.parentElement.classList.toggle('hidden');
            blackOut.classList.toggle('hidden');
        });
    })
</script>

</html>